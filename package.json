    "@openzeppelin/contracts": "^5.0.1",
    "dotenv": "^16.4.5",
    "hardhat": "^2.22.1",
    "hardhat-deploy": "^0.11.45"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat-gas-reporter": "^2.0.0",
    "mocha": "^8.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5"
  },
  "scripts": {
